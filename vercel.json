{
  "version": 2,
  "builds": [
    {
      "src": "api/upload.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "dest": "/api/upload.py"
    },
    {
      "src": "/(.*)",
      "dest": "/static/$1"
    }
  ],
  "functions": {
    "api/upload.py": {
      "maxDuration": 30
    }
  }
}
